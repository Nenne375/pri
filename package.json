{
  "name": "pri",
  "displayName": "Prime Extension",
  "description": "Prime Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "prime",
      "aliases": ["Prime", "prime"],
      "extensions": [".pri"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "prime",
      "scopeName": "source.pri",
      "path": "./syntaxes/prime.tmLanguage.json"
    }]
  }
}
